{
  "name": "aqueduct-twamm",
  "description": "Core contracts for Auwduct's Time-Weighted Auotmated Market Maker (TWAMM). Bootstrapped from a uniswap v2 fork with solidity 0.8",
  "version": "1.0.0",
  "homepage": "https://www.aqueduct.fi/",
  "repository": {
    "type": "git",
    "url": "https://github.com/aqueduct-finance/aqueduct-twamm"
  },
  "keywords": [
    "ethereum",
    "uniswap-v2",
    "aqueduct",
    "superfluid",
    "TWAMM"
  ],
  "files": [
    "contracts",
    "build",
    "typechain-types"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.2",
    "@openzeppelin/contracts": "4.9.2",
    "@superfluid-finance/ethereum-contracts": "1.7.1",
    "@superfluid-finance/sdk-core": "0.6.6",
    "@typechain/ethers-v5": "^10.1.1",
    "@typechain/hardhat": "^6.1.4",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.2",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-preprocessor": "^0.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-rc.1",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "lint": "prettier --check test/**/*.ts script/**/*.ts src/**/*.sol && solhint -f table 'src/**/*.sol'",
    "lint:fix": "prettier --write test/**/*.ts script/**/*.ts src/**/*.sol",
    "clean": "rimraf artifacts cache types",
    "precompile": "yarn clean",
    "build:foundry": "forge build",
    "build:hardhat": "hardhat compile",
    "test:foundry": "forge test",
    "test:hardhat": "hardhat test",
    "node:foundry": "anvil",
    "node:hardhat": "hardhat node",
    "prepublishOnly": "yarn test"
  },
  "license": "GPL-3.0-or-later",
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "graphql": "^16.7.1"
  }
}
